<h1>Listing actorlists</h1>

<table>
  <tr>
    <th>Dvd</th>
    <th>Actor</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @actorlists.each do |actorlist| %>
  <tr>
    <td><%= actorlist.dvd_id %></td>
    <td><%= actorlist.actor_id %></td>
    <td><%= link_to 'Show', actorlist %></td>
    <td><%= link_to 'Edit', edit_actorlist_path(actorlist) %></td>
    <td><%= link_to 'Destroy', actorlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Actorlist', new_actorlist_path %>
