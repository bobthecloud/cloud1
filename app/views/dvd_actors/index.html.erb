<h1>Listing dvd_actors</h1>

<table>
  <tr>
    <th>Dvd</th>
    <th>Actor</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dvd_actors.each do |dvd_actor| %>
  <tr>
    <td><%= dvd_actor.dvd_id %></td>
    <td><%= dvd_actor.actor_id %></td>
    <td><%= link_to 'Show', dvd_actor %></td>
    <td><%= link_to 'Edit', edit_dvd_actor_path(dvd_actor) %></td>
    <td><%= link_to 'Destroy', dvd_actor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dvd actor', new_dvd_actor_path %>
